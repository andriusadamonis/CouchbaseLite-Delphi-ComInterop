<Window x:Class="CouchbaseMobileWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CouchbaseMobileWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <Button Content="Insert" Click="InsertDocumentClick"></Button>
        <Button Content="Update" Click="UpdateDocumentClick"></Button>
        <Button Content="Read" Click="GetDocumentClick"></Button>
        <Button Content="Delete" Click="DeleteDocumentClick"></Button>
        <Button Content="Stop Sync" Click="StopSyncClick"></Button>
        <Button Content="Start Sync" Click="StartSyncClick"></Button>
        <TextBox Name="id" Text="beer_aa1"/>
    </StackPanel>
</Window>
